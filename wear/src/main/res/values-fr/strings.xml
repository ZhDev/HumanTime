<resources>

    <!-- The time MUST contain at most 2 spaces so it can be broken later into 2 or 3 sentences.
    Use '\u00A0' instead to insert non-breaking spaces in numbers and extra spaces in the sentences.
    It should look like <hours> + space + <time_construction> + space + <minutes>, where <hours>,
    <time_construction> and <minutes> don't contain any spaces, only '\u00A0'.-->

    <string name="one">une</string>
    <string name="two">deux</string>
    <string name="three">trois</string>
    <string name="four">quatre</string>
    <string name="five">cinq</string>
    <string name="six">six</string>
    <string name="seven">sept</string>
    <string name="eight">huit</string>
    <string name="nine">neuf</string>
    <string name="ten">dix</string>
    <string name="eleven">onze</string>
    <string name="twelve">douze</string>

    <plurals name="hours">
        <item quantity="one">%s\u00A0heure</item>
        <item quantity="other">%s\u00A0heures</item>
    </plurals>

    <string name="o_clock">{hours}</string>
    <string name="five_past">{hours} cinq</string>
    <string name="ten_past">{hours} dix</string>
    <string name="quarter_past">{hours} et quart</string>
    <string name="twenty_past">{hours} vingt</string>
    <string name="twenty_five_past">{hours} vingt-cinq</string>
    <string name="half_past">{hours} et demie</string>
    <string name="twenty_five_to">{hours} moins vingt-cinq</string>
    <string name="twenty_to">{hours} moins vingt</string>
    <string name="quarter_to">{hours} moins\u00A0le quart</string>
    <string name="ten_to">{hours} moins dix</string>
    <string name="five_to">{hours} moins cinq</string>

</resources>